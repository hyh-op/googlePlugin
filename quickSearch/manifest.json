{
  "manifest_version": 2,
  "name": "Quick Search",
  "version": "1.0",
  "description": "A quick search tool for selected text",
  "background": {
    "scripts": ["background.js"]
  },
  "permissions": ["contextMenus"],
  "content_scripts": [{
    "matches": ["<all_urls>"],
    "js": ["content.js"]
  }]
}
